<?xml version="1.0" encoding="ISO-8859-1"?>
<SIMPLE NAME="MEM/WB Latch" PREFERREDNUMBERFORMAT="Hexadecimal" PREFERREDCYCLELENGTH="10">
    <PARAMETERS/>
    <FORMS>
        <RECTANGLE COLOR="#00A000" POSX="0.0" POSY="-300.0" WIDTH="10.0"
            HEIGHT="60.0" RADIUSX="0.0" RADIUSY="0.0">
            <STROKE/>
            <FILL COLOR="#00A000"/>
        </RECTANGLE>
        <RECTANGLE COLOR="#0000C0" POSX="0.0" POSY="-235.0" WIDTH="10.0"
            HEIGHT="270.0" RADIUSX="0.0" RADIUSY="0.0">
            <STROKE/>
            <FILL COLOR="#0000BE"/>
        </RECTANGLE>
    </FORMS>
    <IO>
        <INPUT BITS="1" SIGNED="false" POSX="0.0" POSY="-290.0" NAME="RegWriteIn">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <INPUT BITS="1" SIGNED="false" POSX="0.0" POSY="-250.0" NAME="MemToRegIn">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <INPUT BITS="16" SIGNED="false" POSX="0.0" POSY="-55.0" NAME="In1">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <INPUT BITS="16" SIGNED="false" POSX="0.0" POSY="10.0" NAME="In2">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <INPUT BITS="4" SIGNED="false" POSX="0.0" POSY="20.0" NAME="In3">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <OUTPUT BITS="1" SIGNED="false" POSX="10.0" POSY="-290.0" NAME="RegWriteOut">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="1" SIGNED="false" POSX="10.0" POSY="-250.0" NAME="MemToRegOut">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="16" SIGNED="false" POSX="10.0" POSY="-55.0" NAME="Out1">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="16" SIGNED="false" POSX="10.0" POSY="10.0" NAME="Out2">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
        <OUTPUT BITS="4" SIGNED="false" POSX="10.0" POSY="20.0" NAME="Out3">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
    </IO>
    <MEMORY>
        <STORAGE NAME="FirstCycle" SIZE="1" BITS="1" SIGNED="false"/>
    </MEMORY>
    <INTERNALS DELAY="4">
        <ACTION EVENT="CLOCK_RISING">
			if (FirstCycle[0])
		{
			RegWriteOut = 0;
			MemToRegOut = 0;
			Out1 = 0;
			Out2 = 0;
			Out3 = 0;
			FirstCycle[0] = 0;
		}
		else
		{
			RegWriteOut = RegWriteIn;
			MemToRegOut = MemToRegIn;
			Out1 = In1;
			Out2 = In2;
			Out3 = In3;
		}
		</ACTION>
    </INTERNALS>
</SIMPLE>
